@model IEnumerable<Campeonato.Models.Resultado>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.QuantidadePontos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEquipeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.QuantidadePontos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEquipeNavigation.IdEquipe)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdResultado">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdResultado">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdResultado">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
